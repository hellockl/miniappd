        </view>
        <view class="card-inputlabel">
          <view class="label">额度配注</view>
          <block wx:if="{{quota_remarks}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">抵押率</view>
          <block wx:if="{{mortgage_rate}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
