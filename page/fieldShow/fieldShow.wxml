          <block wx:if="{{repayment_method}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">还款方式配注</view>
          <block wx:if="{{repayment_method_remarks}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">还款日期</view>
          <block wx:if="{{repayment_date}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">贷款利率</view>
          <block wx:if="{{loan_rate}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">贷款利率配注</view>
          <block wx:if="{{loan_rate_remarks}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">逾期罚息</view>
          <block wx:if="{{late_charge}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">保证金</view>
          <block wx:if="{{deposit}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">有无公正</view>
          <block wx:if="{{is_justice}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">公证费</view>
          <block wx:if="{{justice_fee}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">有无评估</view>
          <block wx:if="{{is_evaluate}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
