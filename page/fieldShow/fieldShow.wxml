        <view class="card-inputlabel">
          <view class="label">包装内容</view>
          <block wx:if="{{package_content}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
            <switch bindchange="switch1Change"/>
          </block>
        </view>
        <view class="card-inputlabel">
          <view class="label">所需材料</view>
          <block wx:if="{{required_materials}}">
            <switch checked bindchange="switch1Change"/>
          </block>
          <block wx:else>
