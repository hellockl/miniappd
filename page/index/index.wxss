/**index.wxss**/
page{
  background-color: #fff; 
}
.btn_add{
  font-size:10pt;
  color: #03a5ef;
  width: 70pt;
  background: #fff;
  margin-left:36rpx;
  border-color: #03a5ef;
}
button[type="primary"][plain] {
  color:#03a5ef;
  border:1px solid #03a5ef;
  
  background-color:transparent;
  margin-bottom:14rpx 

}
.top{
  height: 150rpx;
  background: #fff;
  border-bottom: 1px solid #f0eff5;
}
.info{
  width: 65%;
  border-right: 1px solid #f0eff5;
  float: left;
  display:flex;
  align-items:center;

}
.info-text{
  margin-left: 30rpx;
}
.headPic-img{
  flex:0 0 140rpx;
  position:relative;
  width:140rpx;
  height:140rpx;
  margin-left: 20rpx
  

}
.call{
  width: 32%;
  float: left;
  margin-top:30rpx; 
  
}
.call-text{
  width:80px;
  position:absolute;
  margin-top:5px;
  margin-right:5px;

}
.call-img{
  width: 60rpx;
  height: 60rpx;
  margin-top:10rpx; 
  margin-left:20rpx ;
}
.headPic{
  width:100%;
  height:100%;
  border-radius: 50%;
  border: 1px solid #f0eff5;
}

.fd-bd-title {
    padding-left: 20rpx;
    margin-top: 40rpx;
    
    font-size: 28rpx;
    font-weight: bold;
    color: #444;
    border-left: 6rpx solid #03a5ef;
}
.fd-bd {
    padding: 0 20rpx 2rpx;
}

.banner-view{
  width: 100%;
  height: 410rpx;
  border-bottom: 1px solid #f0eff5
}
.messages{
  width:100rpx;
  height:100rpx;
  position: absolute;
  top:33%;
  right:5%
}
.messag{
  width: 100%;
  height:100%;
  z-index: 999px
}
.msgs{
  display: inline-block;
  width: 70rpx;
  height: 50rpx;
  background:red;
  text-align: center;
  line-height: 50rpx;
  border-radius: 40rpx;
  position: absolute;
  top: 31%;
  right:2%;
  font-size: 30rpx;
  color: #fff
}
.banner-view image {
    width: 100%;
    height:410rpx;
}
.swiper_container {
  height: 60rpx;
  width: 62vw;
}

.swiper_item {
  font-size: 28rpx;
  line-height: 60rpx;
  font-weight:500;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  letter-spacing: 2px;
  color: #888888;
}

.notice{
  background-color: #fff;
  height: 2rem;
}
.notice-image{

  width: 20px;
  height: 16px;
  margin: 9px 0 0 20px ;
}

.left_n{
  width: 20%;
  float: left;
}
.right_n{
  width: 80%;
  float: left;
}
.hot-product{
  margin-top: 10px;
  width: 112px;
  height: 30px;
}

.product_list{
  clear: both;
  margin-top:10rpx; 
  padding-top:10rpx; 
  background-color: #fff;
  height: 100%;
}
.p_header_title{
  text-align: center;
}
.product_item{
  width: 90%;
  height: 235rpx;
  margin: 2rpx auto 14rpx;
  
